{
    "manifest_version": 2,
    "name": "YT-Hide",
    "version": "1.0",
    "description": "Hide videos by channel, url, or title.",
    "permissions": [
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "web_accessible_resources": ["web.js"]
}
